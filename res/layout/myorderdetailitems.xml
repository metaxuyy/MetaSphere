<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:paddingBottom="5.0dip"
    android:paddingTop="5.0dip" >

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:paddingLeft="8.0dip"
        android:paddingRight="8.0dip" >

        <TextView
            android:id="@+id/textSkuIcon"
            style="@style/style_13_808183_BOLD"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:text="@string/menu_lable_130" />

        <TextView
            android:id="@+id/textProductcode"
            style="@style/style_13_808183_BOLD"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="88.0dip"
            android:gravity="right"
            android:maxWidth="200.0dip" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:paddingLeft="8.0dip"
        android:paddingRight="8.0dip" >

        <TextView
            android:id="@+id/textNameIcon"
            style="@style/style_13_808183_BOLD"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:text="@string/menu_lable_131" />

        <TextView
            android:id="@+id/textName"
            style="@style/style_13_808183_BOLD"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="88.0dip"
            android:gravity="right"
            android:maxWidth="200.0dip"
            android:singleLine="true" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:paddingLeft="8.0dip"
        android:paddingRight="8.0dip" >

        <TextView
            android:id="@+id/textUnitIcon"
            style="@style/style_13_808183_BOLD"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:text="@string/menu_lable_132" />

        <TextView
            android:id="@+id/textUnit"
            style="@style/style_13_808183_BOLD"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5.0dip"
            android:layout_toRightOf="@id/textUnitIcon"
            android:gravity="right"
            android:maxWidth="200.0dip" />

        <TextView
            android:id="@+id/textSizeIcon"
            style="@style/style_13_808183_BOLD"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="88.0dip"
            android:text="@string/menu_lable_133" />

        <TextView
            android:id="@+id/textSize"
            style="@style/style_13_808183_BOLD"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5.0dip"
            android:layout_toRightOf="@id/textSizeIcon"
            android:gravity="right"
            android:maxWidth="200.0dip" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:paddingLeft="8.0dip"
        android:paddingRight="8.0dip" >

        <TextView
            android:id="@+id/textAumountIcon"
            style="@style/style_13_808183_BOLD"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:text="@string/mybill_list_lable_8" />

        <TextView
            android:id="@+id/textAumount"
            style="@style/style_13_808183_BOLD"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5.0dip"
            android:layout_toRightOf="@id/textAumountIcon"
            android:gravity="right"
            android:maxWidth="200.0dip" />

        <TextView
            android:id="@+id/textPriceIcon"
            style="@style/style_13_808183_BOLD"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="88.0dip"
            android:text="@string/mybill_list_lable_6" />

        <TextView
            android:id="@+id/textPrice"
            style="@style/style_14_C21513_BOLD"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5.0dip"
            android:layout_toRightOf="@id/textPriceIcon"
            android:gravity="right"
            android:maxWidth="200.0dip" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rldients"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:paddingLeft="8.0dip"
        android:paddingRight="8.0dip"
        android:visibility="gone" >

        <TextView
            android:id="@+id/textDients"
            style="@style/style_13_808183_BOLD"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:ellipsize="marquee"
            android:singleLine="true"
            android:text="@string/mybill_list_lable_8" />
    </RelativeLayout>

</LinearLayout>