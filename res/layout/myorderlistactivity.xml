<?xml version="1.0" encoding="UTF-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/home_backgroud" >

    <RelativeLayout
        android:id="@+id/LinearLayout01"
        android:layout_width="fill_parent"
        android:layout_height="44.0dip"
        android:layout_gravity="top"
        android:background="@drawable/logo_bg"
        android:gravity="center_vertical"
        android:paddingLeft="8.0dip"
        android:paddingRight="8.0dip" >

        <FrameLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true" >

            <TextView
                android:id="@+id/TextView01"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:shadowColor="#75000000"
                android:shadowDy="2.0"
                android:shadowRadius="2.0"
                android:text="@string/order_mine_infos"
                android:textColor="#ffffffff"
                android:textSize="20.0sp" />
        </FrameLayout>
    </RelativeLayout>

    <LinearLayout
        android:id="@+id/linButton"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/LinearLayout01"
        android:background="@drawable/segment_bg"
        android:gravity="center"
        android:orientation="horizontal"
        android:paddingBottom="5.0dip"
        android:paddingTop="5.0dip" >

        <TextView
            android:id="@+id/textCate_notsend"
            style="@style/style_13_FFFFFF_BOLD"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/segment_selected_1_bg"
            android:gravity="center"
            android:paddingBottom="5.0dip"
            android:paddingLeft="8.0dip"
            android:paddingRight="8.0dip"
            android:paddingTop="5.0dip"
            android:text="@string/menu_lable_120" />

        <TextView
            android:id="@+id/textCate_mounth"
            style="@style/style_13_4B4B4B_BOLD"
            android:layout_width="100.0dip"
            android:layout_height="wrap_content"
            android:background="@drawable/segment_normal_2_bg"
            android:gravity="center"
            android:paddingBottom="5.0dip"
            android:paddingLeft="8.0dip"
            android:paddingRight="8.0dip"
            android:paddingTop="5.0dip"
            android:text="@string/menu_lable_121" />

        <TextView
            android:id="@+id/textCate_all"
            style="@style/style_13_4B4B4B_BOLD"
            android:layout_width="100.0dip"
            android:layout_height="wrap_content"
            android:background="@drawable/segment_normal_3_bg"
            android:gravity="center"
            android:paddingBottom="5.0dip"
            android:paddingLeft="8.0dip"
            android:paddingRight="8.0dip"
            android:paddingTop="5.0dip"
            android:text="@string/menu_lable_122" />
    </LinearLayout>

    <ListView
        android:id="@+id/listOrder"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_below="@id/linButton"
        android:layout_marginBottom="10.0dip"
        android:layout_marginLeft="10.0dip"
        android:layout_marginRight="10.0dip"
        android:layout_marginTop="10.0dip"
        android:background="@null"
        android:cacheColorHint="#00000000"
        android:divider="@null"
        android:dividerHeight="10.0dip"
        android:fadingEdgeLength="1.0dip"
        android:footerDividersEnabled="false"
        android:headerDividersEnabled="true"
        android:listSelector="@anim/zhifubaolist_bg"
        android:visibility="gone" />

    <TextView
        android:id="@+id/textOrderNull"
        style="@style/style_18_666666"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:gravity="center"
        android:text="@string/menu_lable_123"
        android:visibility="gone" />

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent" >

        <ProgressBar
            android:id="@+id/probar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:indeterminateDrawable="@drawable/my_progressbar_bg_color" />
    </RelativeLayout>

</RelativeLayout>